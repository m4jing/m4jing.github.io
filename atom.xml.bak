layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

 <title>Jing Ma</title>
 <link href="http://www.imajing.com.cn/atom.xml" rel="self"/>
 <link href="http://www.imajing.com.cn/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://www.imajing.com.cn/</id>
 <author>
   <name>Jing Ma</name>
   <email>mjingm87@qq.com</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://www.imajing.com.cn{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://www.imajing.com.cn{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}

</feed>